{
  "name": "SoundCloud Scrobbler",
  "description": "Chrome Extension that submits to Last.fm (scrobble) music you play on SoundCloud",
  "version": "1.2",
  "permissions": [
    "tabs",
    "storage",
    "https://soundcloud.com/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [ {
    "matches": [ "https://soundcloud.com/*" ],
    "css": [ "style/style.css" ],
    "js": [
      "library/jquery.min.js",
      "library/mustache.js",
      "library/md5.js",
      "script/system.js",
      "script/index.js"
    ]
  } ],
  "web_accessible_resources": [
    "image/*.png",
    "style/*.css",
    "library/*.js",
    "script/*.js",
    "template/*.mst"
  ],
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "minimum_chrome_version": "20",
  "manifest_version": 2
}
